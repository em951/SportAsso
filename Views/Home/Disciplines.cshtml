@model IEnumerable<SportAssovv.Models.Discipline>

@{
    ViewBag.Title = "Disciplines";
}

<h2>Disciplines</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomDiscipline)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomDiscipline)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DisciplineId }) |
            @Html.ActionLink("Details", "Details", new { id=item.DisciplineId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DisciplineId })
        </td>
    </tr>
}

</table>
