@model IEnumerable<SportAssovv.Models.Discipline>

@{
    ViewBag.Title = "ListeDisciplines";
}

<h2>ListeDisciplines</h2>

<table class="table">
    <tr>
        <th>
           Discipline
        </th>
        <th>
            Places Disponibles
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomDiscipline)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PlacesDisponibles)
        </td>
    </tr>
}

</table>
